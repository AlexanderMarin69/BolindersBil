<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <link href="https://fonts.googleapis.com/css?family=Open+Sans|Raleway&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Cabin&display=swap" rel="stylesheet">

    <link href="~/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/lib/animate/animate.css" rel="stylesheet" />
    <link href="~/lib/ionicons/css/ionicons.css" rel="stylesheet" />
    <link href="~/lib/owlcarousel/assets/owl.carousel.css" rel="stylesheet" />
    <link href="~/lib/lightbox/css/lightbox.css" rel="stylesheet" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.2/css/solid.css" integrity="sha256-gpZL4O9v1sMxFqxDr+1zEsM5geg+qckpVQcMMEWIuRw=" crossorigin="anonymous" />

    <link href="~/maincss/main.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />

    @*<link href="~/twitter-bootstrap/css/bootstrap.css" rel="stylesheet" />*@

</head>
<body>
    <br />
    <br />
    <header id="header" class="fixed-top">
            <div class="container">

                <div class="logo float-left">
                    <!-- Uncomment below if you prefer to use an image logo -->
                    <!-- <h1 class="text-light"><a href="#header"><span>NewBiz</span></a></h1> -->
                    <a href="#intro" class="scrollto">
                        <div class="custom-logo-style">Bolinders Bil AB</div>
                    </a>

                </div>



              

            </div>
        </header><!-- #header -->

    <br />
    <br />
    <br />





    <div class="container body-content">

        <div>@RenderBody()</div>

        @*<hr />*@




        <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>


    </div>
    <script src="~/jquery/jquery.js"></script>
    <script src="~/lib/jquery/jquery-migrate.min.js"></script>
    <script src="~/twitter-bootstrap/js/bootstrap.js"></script>
    <script src="~/lib/easing/easing.js"></script>
    <script src="~/lib/mobile-nav/mobile-nav.js"></script>
    <script src="~/lib/wow/wow.js"></script>
    <script src="~/lib/waypoints/waypoints.min.js"></script>
    <script src="~/lib/counterup/counterup.min.js"></script>
    <script src="~/lib/owlcarousel/owl.carousel.js"></script>
    <script src="~/lib/isotope/isotope.pkgd.js"></script>
    <script src="~/lib/lightbox/js/lightbox.js"></script>
    <script src="~/js/main.js"></script>
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>


    <script src="~/js/BulkDelete.js"></script>
    <script src="~/js/imgGallery.js"></script>


</body>
</html>
