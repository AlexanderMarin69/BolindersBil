@model List<FileUpload>







<div class="col-lg-6">
    <div id="carouselCarImageSlider" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carouselCarImageSlider" data-slide-to="0" class="active"></li>
            <li data-target="#carouselCarImageSlider" data-slide-to="1"></li>
            <li data-target="#carouselCarImageSlider" data-slide-to="2"></li>
            <li data-target="#carouselCarImageSlider" data-slide-to="3"></li>
            <li data-target="#carouselCarImageSlider" data-slide-to="4"></li>
            <li data-target="#carouselCarImageSlider" data-slide-to="5"></li>
            <li data-target="#carouselCarImageSlider" data-slide-to="6"></li>
            <li data-target="#carouselCarImageSlider" data-slide-to="7"></li>
            <li data-target="#carouselCarImageSlider" data-slide-to="8"></li>
            <li data-target="#carouselCarImageSlider" data-slide-to="9"></li>
        </ol>


        @*<div class="carousel-inner">
           
                <div class="carousel-item active">
                    <img class="d-block w-100" src="" alt="">
                </div>

            
        </div>*@


        <div class="carousel-inner">
            @foreach (var carImage in Model)
            {
                <div class="carousel-item active">
                    <img class="d-block w-100" src="@carImage.FilePath" alt="">
                </div>

            }
        </div>








        <a class="carousel-control-prev" href="#carouselCarImageSlider" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselCarImageSlider" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>